{
  "source": "./contents",
  "metadata": {
    "name": "QABC",
    "description": "The Queensland Amateur Brewing Competition",
    "committee": [
      {
        "position": "Organiser & Competition Secretary:",
        "holder": "Mark Davies"
      },
      {
        "position": "Judge Director & Head Steward:",
        "holder": "Tony Brown"
      },
      {
        "position": "Registrar:",
        "holder": "Jason McDonald"
      },
      {
        "position": "Transportation:",
        "holder": "Dean Tummers"
      }
    ]
  },
  "plugins": {
    "metalsmith-markdown": {},
    "metalsmith-json": {},
    "metalsmith-collections": {
      "sponsors": {
        "pattern": "sponsors/*.json"
      },
      "results": {
        "pattern": "results/*.html"
      }
    },
    "metalsmith-layouts": {
      "engine": "handlebars",
      "directory": "./templates",
      "partials": {
        "layout-home": "partials/layout-home",
        "layout-article": "partials/layout-article",
        "head": "partials/head",
        "header": "partials/header",
        "footer": "partials/footer"
      }
    },
    "metalsmith-discover-partials": {
      "directory": "partials",
      "pattern": "/\\.hbs$/"
    },
    "metalsmith-paths": {},
    "metalsmith-debug": {}
  }
}
